#include <stdio.h>
#include <stdlib.h>
#define boyut 15

/*Donguler araciligiyla, yildizlarla ekrana e harfini yazdiran C dilinde yazilmis kod parcasi.*/

int main()
{
    int i,j;
    int bosluk = 0;

    for(i=1; i<=boyut+2; i++)
    {
        for(j=1; j<=boyut; j++)
        {
            printf("*");
        }
        printf("\n");

        for (i=1; i<=boyut/2-1; i++)
        {
            printf("*");
            printf("\n");
            bosluk = boyut-1;
        }
        break;
    }
    for(i=1; i<=boyut+2; i++)
    {
        for(j=1; j<=boyut; j++)
        {
            printf("*");
        }
        printf("\n");

        for (i=1; i<=boyut/2-1; i++)
        {
            printf("*");
            printf("\n");
            bosluk = boyut-1;
        }
        break;
    }
    for(i=1; i<=boyut+2; i++)
    {
        for(j=1; j<=boyut; j++)
        {
            printf("*");
        }
        break;
    }
      return 0;
}
