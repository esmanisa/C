#include <stdio.h>
#include <math.h>
#define pi 3.14

/*Kullanicidan alinan aci derecesine gore sin√ºs ve cosinus degerlerini hesaplayan C dilinde yazilmis kod.*/

int main()
{
float x;
printf("Aciyi derece cinsinden giriniz...: "); scanf("%f",&x);
printf("sin %.2f = %f \n",x,sin(x*pi/180)); // fonk icindeki ifade girilen aciyi radyana cevirir
printf("cos %.2f = %f",x,cos(x*pi/180));
return 0;
}
