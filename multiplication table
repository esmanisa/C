#include <stdio.h>
#include <stdlib.h>

/*1'den 10'a kadar olan sayilarin carpim tablosunu yazan C dilinde yazilmis kod parcasi.*/

int main()
{
        int i, j;
        for (i=1; i<10; i++)
        {
            for (j=1; j<=10; j++)
            {
                printf("%d * %2d = %2d \n", i,j,i*j);
            }
            printf("--------\n");
        }


        return 0;
}
